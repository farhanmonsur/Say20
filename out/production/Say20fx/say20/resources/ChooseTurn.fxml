<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="choose_turn" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="say20.ChooseTurnController">
    <Label layoutX="196.0" layoutY="80.0" text="Do you want to say first?" textAlignment="CENTER">
        <font>
            <Font size="19.0" />
        </font>
    </Label>
    <Button layoutX="274.0" layoutY="143.0" mnemonicParsing="false" onAction="#YesButtonPushed" prefHeight="25.0" prefWidth="51.0" text="Yes" />
    <Button layoutX="274.0" layoutY="200.0" mnemonicParsing="false" onAction="#NoButtonPushed" prefHeight="25.0" prefWidth="51.0" text="No" textAlignment="CENTER" />
    <Button layoutX="270.0" layoutY="270.0" mnemonicParsing="false" onAction="#GoBackButtonPushed" text="Go Back" />
</AnchorPane>
